<p id="notice"><%= notice %></p>

<h1>Task Completes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @completed_tasks.each do |completed_task| %>
      <tr>
        <td><%= link_to 'Show', completed_task %></td>
        <td><%= link_to 'Edit', edit_task_complete_path(completed_task) %></td>
        <td><%= link_to 'Destroy', completed_task, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Task Complete', new_completed_task_path %>
